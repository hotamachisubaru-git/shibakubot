{
  "name": "discord-bot",
  "version": "1.2.0",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "node scripts/clean-dist.mjs && tsc",
    "start": "node dist/index.js",
    "register": "ts-node src/deploy-commands.ts",
    "register:prod": "node dist/deploy-commands.js",
    "migrate": "ts-node scripts/migrate-old-data.ts",
    "release:bundle": "npm run build && node scripts/create-release-bundle.mjs"
  },
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "express": "^5.2.1",
    "lavalink-client": "^2.7.0",
    "music-metadata": "^11.10.5"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.6",
    "@types/node": "^22.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20"
  }
}
